<view class="page">
		<view class="page__bd">
			<form bindsubmit="formSubmit">
				<view class="profile-info-wrap">
					<view class="cell cell_input">
						<view class="cell__hd">
								<view class="label">用户昵称</view>
						</view>
						<view class="cell__bd">
								<input class="input" name="nickname" placeholder="请输入姓名"/>
						</view>
					</view>
					<view class="cell cell_input">
						<view class="cell__hd">
								<view class="label">联系方式</view>
						</view>
						<view class="cell__bd">
								<input class="input" name="contact" placeholder="请输入联系方式"/>
						</view>
					</view>
					<view class="cell">
						<view class="cell__hd">
							<view class="label">反馈内容</view>
						</view>
						<view class="cell__bd">
								<textarea name="content" class="textCenter textarea" placeholder="请输入反馈内容" />
								<!-- <view class="textarea-counter">0/200</view> -->
						</view>
					</view>
				</view>
				<view class="btn-area confirm-profile">
						<button formType="submit" type="primary">提交</button>
				</view>
			</form>
		</view>
</view>
